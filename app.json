{
  "expo": {
    "name": "Meistru",
    "version": "1.1.0",
    "description": "Meistru permette ai direttori di coro di salvare le note iniziali di ogni reparto, organizzare i concerti ed eseguire l'accordo iniziale.",
    "slug": "meistru",
    "owner": "belinde",
    "privacy": "unlisted",
    "platforms": ["android"],
    "orientation": "portrait",
    "githubUrl": "https://github.com/belinde/meistru",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "dark",
    "backgroundColor": "#242B1F",
    "primaryColor": "#386b01",
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#242B1F"
    },
    "androidNavigationBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#242B1F"
    },
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#242B1F"
    },
    "assetBundlePatterns": ["**/*"],
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#242B1F"
      },
      "package": "click.belinde.meistru"
    },
    "extra": {
      "eas": {
        "projectId": "c4bb382c-6aaf-40e0-a2bb-75897ffdee8a"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/c4bb382c-6aaf-40e0-a2bb-75897ffdee8a"
    },
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "enableProguardInReleaseBuilds": true,
            "extraProguardRules": "-keep public class com.horcrux.svg.** {*;}",
            "enableShrinkResourcesInReleaseBuilds": true,
            "allowBackup": false
          }
        }
      ]
    ]
  }
}
